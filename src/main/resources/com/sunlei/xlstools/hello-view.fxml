<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.TilePane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" prefHeight="429.0" prefWidth="700.0" spacing="20.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.sunlei.xlstools.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
    <Pane prefHeight="412.0" prefWidth="660.0">
        <children>
            <Button fx:id="selectXlsBtn" layoutX="143.0" layoutY="164.0" mnemonicParsing="false" onAction="#selectXlsClick" prefHeight="42.0" prefWidth="108.0" style="-fx-background-color: green;" text="选择文件:" textFill="#fffbfb" />
            <Label layoutX="262.0" layoutY="164.0" prefHeight="42.0" prefWidth="217.0" fx:id="xlsLabel" />
            <Button fx:id="submitBtn" layoutX="143.0" layoutY="263.0" mnemonicParsing="false" onAction="#submitClick" prefHeight="42.0" prefWidth="360.0" style="-fx-background-color: #38b48b;" text="生成统计文件" textFill="#f8eded" />
         <TilePane layoutX="143.0" layoutY="22.0" prefHeight="134.0" prefWidth="360.0">
            <children>
               <Label alignment="TOP_CENTER" prefHeight="37.0" prefWidth="363.0" style="-fx-text-fill: center;" text="根据XLS文件统计快递数量" textFill="#3c44c7b5">
                  <font>
                     <Font size="27.0" />
                  </font>
               </Label>
            </children>
         </TilePane>
        </children>
    </Pane>
</VBox>
